        "template--15909486657730__9ad871c6-5aa9-4704-a269-b16e6b1fb73b-16727468629c5b3763-1": {
          "type": "text",
          "settings": {
            "text-block-width": 925,
            "text": "<p>All of our profits are now shared equally between our charities.<\/p>",
            "text-font-family": "",
            "text-font-weight": 300,
            "text-font-color": "",
            "text-size": 80,
            "text-line-height": 1,
            "mobile-text-size": 16,
